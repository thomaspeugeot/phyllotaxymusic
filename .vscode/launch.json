{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng-github.com-thomaspeugeot-phyllotaxymusic"
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng-github.com-thomaspeugeot-phyllotaxymusic"
        },
        {
            "name": "go server on music",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/phyllotaxymusic/main.go",
            "args": [
                "-unmarshallFromCode=data/music.go",
                "-marshallOnCommit=data/music",
            ]
        },
        {
            "name": "go server on frecon open ended curve",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/phyllotaxymusic/main.go",
            "args": [
                "-unmarshallFromCode=data/frecon open ended curve.go",
                "-marshallOnCommit=data/frecon open ended curve",
            ]
        },
                {
            "name": "go server on frecon circular curve",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/phyllotaxymusic/main.go",
            "args": [
                "-unmarshallFromCode=data/frecon circular curve.go",
                "-marshallOnCommit=data/frecon circular curve",
            ]
        },

                {
            "name": "go server on music2",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/phyllotaxymusic/main.go",
            "args": [
                "-unmarshallFromCode=data/music2.go",
                "-marshallOnCommit=data/music2",
            ]
        },
        {
            "name": "go server on music, output on integrated terminal",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/phyllotaxymusic/main.go",
            "args": [
                "-unmarshallFromCode=data/music.go",
                "-marshallOnCommit=data/music",
            ],
             "console": "integratedTerminal"
        },
    ]
}